<% provide(:title, "Add new tag") %>
<h1>新しいタグを追加する</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @tag do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class:'form-control'%>
    <%= f.submit "タグを追加", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
